{
  "name": "release-ninja",
  "version": "0.1.0",
  "description": "Release apk, ipa and other binaries in a fast and managed way",
  "main": "index.js",
  "scripts": {
    "lint": "eslint server",
    "dev": "node server/server.js",
    "build": "next build",
    "start": "next start",
    "test": "jest --coverage --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianSo/release-ninja.git"
  },
  "keywords": [
    "release",
    "apk",
    "ipa",
    "android",
    "ios"
  ],
  "author": "Brian So",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/BrianSo/release-ninja/issues"
  },
  "homepage": "https://github.com/BrianSo/release-ninja#readme",
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "testPathIgnorePatterns": ["<rootDir>/src/.next/", "<rootDir>/node_modules/"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "server/**/*.js",
      "!src/.next/**/*.js",
      "!server/utils/migrations/**/*.js",
      "!server/utils/seeders/**/*.js"
    ],
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "connect-compose": "^0.0.1",
    "connect-mongo": "^2.0.1",
    "csurf": "^1.9.0",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "htmlescape": "^1.1.1",
    "lodash": "^4.17.10",
    "lusca": "^1.6.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mongoose": "^5.2.5",
    "morgan": "^1.9.0",
    "next": "^6.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "chokidar": "^2.0.4",
    "eslint": "^5.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "jest": "^23.4.2",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": ">=8.10.0"
  }
}
