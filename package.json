{
  "name": "release-ninja",
  "version": "0.1.0",
  "description": "Release apk, ipa and other binaries in a fast and managed way",
  "main": "index.js",
  "scripts": {
    "lint": "eslint server",
    "dev": "node server/server.js",
    "build": "next build",
    "start": "next start",
    "test": "yarn test-server && yarn test-client && yarn coverage",
    "test-server": "NODE_ENV=test jest --coverage --forceExit --projects server",
    "test-client": "NODE_ENV=test jest --coverage --forceExit --projects src",
    "coverage": "node scripts/coverage-combine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianSo/release-ninja.git"
  },
  "keywords": [
    "release",
    "apk",
    "ipa",
    "android",
    "ios"
  ],
  "author": "Brian So",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/BrianSo/release-ninja/issues"
  },
  "homepage": "https://github.com/BrianSo/release-ninja#readme",
  "jest": {
    "projects": [
      "src/jest.config.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "connect-compose": "^0.0.1",
    "connect-mongo": "^2.0.1",
    "csurf": "^1.9.0",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-request-language": "^1.1.15",
    "express-session": "^1.15.6",
    "htmlescape": "^1.1.1",
    "i18next": "^11.5.0",
    "joi": "^13.5.2",
    "lodash": "^4.17.10",
    "lusca": "^1.6.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mongoose": "^5.2.5",
    "morgan": "^1.9.0",
    "next": "^6.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.56",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.56",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "chokidar": "^2.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "istanbul": "0.4.5",
    "istanbul-combine": "https://github.com/BrianSo/istanbul-combine",
    "jest": "^23.4.2",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": ">=8.10.0"
  }
}
